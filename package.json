{
  "name": "@onsvisual/sveltekit-starter",
  "homepage": "http://onsvisual.github.io/sveltekit-starter",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && node ./scripts/js-fix",
    "build:preview": "cross-env PUBLIC_APP_ENV=preview vite build",
    "package": "vite package",
    "preview": "vite preview",
    "deploy": "gh-pages -d build -t true",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "author": "Ahmad Barclay",
  "license": "MIT",
  "devDependencies": {
    "@onsvisual/svelte-components": "^0.1.72",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.15.8",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^4.2.2",
    "vite": "^4.3.3"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "type": "module"
}
